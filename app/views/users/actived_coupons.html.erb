<table class="table table-hover">
  <tr class="head">
    <td>所属教练/驾校</td>
    <td>代金券面额</td>
    <td>代金券剩余数量</td>
    <td>教练/驾校评价</td>
    <td style="width:140px"></td>
  </tr>
  
  <% @coupons.each do |coupon| %>
  <tr>
    <td><%= render_ownerable_title(coupon.ownerable) %></td>
    <td><%= coupon.value %></td>
    <td><%= coupon.publish_count - coupon.actives_count %></td>
    <td><%= render_comment_star(coupon.ownerable) %></td>
    <td><%= commentable_tag(coupon.ownerable) %></td>
  </tr>
  <% end %>
</table>